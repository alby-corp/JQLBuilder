<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>JQLBuilder</Title>
        <Authors>Alberto Viezzi &amp; Roald Humar</Authors>
        <PackageProjectUrl>https://github.com/alby-corp/jql-builder</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/alby-corp/jql-builder/blob/main/LICENSE</PackageLicenseUrl>
        <PackageIcon>icon.png</PackageIcon>
        <RepositoryUrl>https://github.com/alby-corp/jql-builder</RepositoryUrl>
        <PackageTags>jql builder alassian</PackageTags>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>1.0.0</FileVersion>
    </PropertyGroup>

    <ItemGroup>
        <None Update="Assert\icon.png">
            <Pack>True</Pack>
        </None>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\JQLBuilder.Types\JQLBuilder.Types.csproj"/>
    </ItemGroup>

</Project>
